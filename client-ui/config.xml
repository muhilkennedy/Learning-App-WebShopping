<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ui.client" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NuttyShop</name>
    <description>
        Client UI - angular to android application for client functionality
    </description>
    <author email="muhilkennedy@gmail.com" href="http://cordova.io">
        Muhil
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
    <icon foreground="src/assets/images/logo/Brand-Icon.png" density="ldpi" background="src/assets/images/logo/Brand-Icon.png"/>
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
